{
  "name": "forex-api-daily",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules index.js",
    "compile": "tsc -p tsconfig.build.json",
    "fix": "pnpm run format && pnpm run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "posttest": "pnpm run quality",
    "pretest": "pnpm run compile",
    "quality": "pnpm run format:check && pnpm run lint && pnpm run typecheck",
    "record": "pnpm compile && firebase emulators:exec --only firestore \"tsx scripts/record.ts\"",
    "start": "firebase emulators:exec --only firestore \"GCP_PROJECT_ID=forex-api-daily functions-framework --target=main --signature-type=http --port=8081\"",
    "test": "firebase emulators:exec --only firestore \"vitest --run\"",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamaal111/forex-api-daily.git"
  },
  "author": "Kamaal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamaal111/forex-api-daily/issues"
  },
  "homepage": "https://github.com/kamaal111/forex-api-daily#readme",
  "dependencies": {
    "@google-cloud/firestore": "^8.0.0",
    "@google-cloud/functions-framework": "^4.0.1",
    "@kamaalio/kamaal": "^0.7.8",
    "cheerio": "^1.1.2",
    "valibot": "^1.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@kamaalio/prettier-config": "^0.1.2",
    "@types/node": "^22",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "firebase-tools": "^14.26.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14",
    "wait-port": "^1.1.0"
  },
  "prettier": "@kamaalio/prettier-config",
  "packageManager": "pnpm@10.24.0"
}
